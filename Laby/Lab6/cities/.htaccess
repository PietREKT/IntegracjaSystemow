RewriteEngine On # Turn on the rewriting engine
RewriteRule ^read$ read.php [NC,L] #Add read mapping
RewriteRule ^read/([0-9_-]*)$ read.php?id=$1 [NC,L]

RewriteRule ^delete/([0-9_-]*)$ delete.php?id=$1 [NC,L]
RewriteRule ^update/([0-9_-]*)$ update.php?id=$1 [NC,L]
RewriteRule ^save$ save.php [NC,L]

